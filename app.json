{
  "expo": {
    "name": "sheep-rn",
    "slug": "sheep-rn",
    "version": "1.5.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4d8744"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.sheeprn",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "compileSdkVersion": 36,
      "targetSdkVersion": 36
    },
    "ios": {
      "bundleIdentifier": "com.sheeprn",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to save database backups"
      }
    },
    "extra": {
      "eas": {
        "projectId": "4d38e588-9de2-4bda-82f4-738515b85b4e"
      }
    }
  }
}